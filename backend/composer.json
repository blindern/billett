{
	"name": "blindernuka/billett",
	"description": "Billettsystemet til UKA p√• Blindern",
	"keywords": ["blindernuka", "billett"],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/henrist/laravel-api-query"
		}
	],
	"require": {
		"laravel/framework": "4.2.*",
		"dompdf/dompdf" : "0.6.*",
		"milon/barcode": "~4.2",
		"intervention/image": "~2.0",
		"iio/libmergepdf": "~2.4",
		"knight-swarm/laravel-saml": "dev-master",
		"henrist/laravel-api-query": "dev-master",
		"doctrine/dbal": "~2.5",
		"barryvdh/laravel-ide-helper": "~1.11",
		"guzzlehttp/guzzle": "^6"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-4": {
			"Blindern\\UKA\\": "app/src"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
