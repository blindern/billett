{
    "name": "blindernuka/billett",
    "description": "Billettsystemet til UKA p√• Blindern",
    "keywords": [
        "blindernuka",
        "billett"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/henrist/laravel-api-query"
        }
    ],
    "require": {
        "php": "8.4.*",
        "dompdf/dompdf": "^3",
        "guzzlehttp/guzzle": "7.10.0",
        "henrist/laravel-api-query": "dev-master",
        "iio/libmergepdf": "4.0.4",
        "intervention/image-laravel": "^1.3",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.9",
        "milon/barcode": "^12",
        "onelogin/php-saml": "4.3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^12.0.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Console/Commands"
        ],
        "files": [
            "app/Helpers/format.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Blindern\\UKA\\": "app/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
